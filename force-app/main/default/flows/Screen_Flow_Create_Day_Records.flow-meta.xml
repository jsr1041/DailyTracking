    <formulas>
        <name>f_Date_Value</name>
        <dataType>Date</dataType>
        <expression>DATE(YEAR({!Starting_Date}), 
IF(
    (DAY({!Starting_Date}+{!var_Counter})&lt;31), 
    MONTH({!Starting_Date}), 
    MONTH({!Starting_Date})+1), 
DAY({!Starting_Date}+{!var_Counter}))</expression>
    </formulas>
    <formulas>
        <name>f_Day_Number</name>
        <dataType>Number</dataType>
        <expression>(DAY({!Starting_Date})) + {!var_Counter}</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_Day_Number_Value</name>
        <dataType>Number</dataType>
        <expression>DAY({!f_Date_Value})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_Next_Month_Number</name>
        <dataType>Number</dataType>
        <expression>{!Get_Current_Month.Month_Number__c}+1</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_Next_Year_Name</name>
        <dataType>String</dataType>
        <expression>TEXT(VALUE({!Get_Current_Month.Year__r.Name})+1)</expression>
    </formulas>
