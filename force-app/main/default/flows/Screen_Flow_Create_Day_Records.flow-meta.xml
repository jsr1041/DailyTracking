            <transformValueActions>
                <outputFieldApiName>Name</outputFieldApiName>
                <transformType>Map</transformType>
                <value>
                    <formulaDataType>String</formulaDataType>
                    <formulaExpression>IF(TEXT({!f_Next_Month_Number})=&quot;1&quot;, &quot;January&quot;, 
    IF(Text({!f_Next_Month_Number})=&quot;2&quot;, &quot;February&quot;,
        IF(Text({!f_Next_Month_Number})=&quot;3&quot;, &quot;March&quot;,
            IF(Text({!f_Next_Month_Number})=&quot;4&quot;, &quot;April&quot;,
                IF(Text({!f_Next_Month_Number})=&quot;5&quot;, &quot;May&quot;,
                    IF(Text({!f_Next_Month_Number})=&quot;6&quot;, &quot;June&quot;,
                        IF(Text({!f_Next_Month_Number})=&quot;7&quot;, &quot;July&quot;,
                            IF(Text({!f_Next_Month_Number})=&quot;8&quot;, &quot;August&quot;,
                                IF(Text({!f_Next_Month_Number})=&quot;9&quot;, &quot;September&quot;,
                                    IF(Text({!f_Next_Month_Number})=&quot;10&quot;, &quot;October&quot;,
                                        IF(Text({!f_Next_Month_Number})=&quot;11&quot;, &quot;November&quot;, 
                                            IF(Text({!f_Next_Month_Number})=&quot;12&quot;, &quot;December&quot;, &quot;Fix this month&quot;
                                                ))))))))))))</formulaExpression>
                </value>
            </transformValueActions>
